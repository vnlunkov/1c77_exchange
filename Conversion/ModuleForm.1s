		ВрмИмяУзла = СтрЗаменить(ВрмУзелПараметр.nodeName, "m:", "");
		
		Если Найти(ВрмИмяУзла, "return") > 0 Тогда
			ВрмВозврат.Установить("СтатусВозврата", 1);
			Продолжить;
		ИначеЕсли Найти(ВрмИмяУзла, "fault") > 0 Тогда
			ВрмВозврат.Установить("ОписаниеОшибки", ВрмУзелПараметр.text);
			Прервать;
		КонецЕсли;

		ВрмВозврат.Установить(ВрмИмяУзла, ВрмУзелПараметр.text);

