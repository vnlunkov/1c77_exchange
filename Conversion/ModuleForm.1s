			Возврат 0;
			
		КонецЕсли;
		
	КонецЕсли;
	
	// мТаблицаПравилВыгрузки.ВыбратьСтроки();
	
	// Пока мТаблицаПравилВыгрузки.ПолучитьСтроку() > 0 Цикл
		
	// 	Префикс = "";
		
	// 	Если мТаблицаПравилВыгрузки.ЭтоГруппа = 1 Тогда
			
