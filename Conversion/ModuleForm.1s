Процедура ЗакончитьЧтениеСообщения(ВхУзелОбменаИдентификатор, ВхПакетНомер, ВрмНомерПринятогоКорреспондентом)

	//ВрмЗапросСКЛ = ПолучитьCOMОбъект("ODBCRecordset");

	ВрмТекстЗапроса = "
		|UPDATE _1SDWNLDS
		|	SET ACKNOWL = 'A'
		|FROM  _1SDWNLDS T_DWNLDS
		|INNER JOIN _1SSYSTEM T_SYSTEM ON T_DWNLDS.DIRECT == 'O' AND T_DWNLDS.DBSIGN == RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3)
		|AND T_DWNLDS.DWNLDID <= RIGHT('      ' + RTRIM('" + _IdToStr(ВрмНомерПринятогоКорреспондентом) + "'), 6) + T_SYSTEM.DBSIGN";
		//|AND T_DWNLDS.DWNLDID == 'FFFFFFFFF'";
		ВрмСтатусВозврата = ВыполнитьЗапросБД(ВрмТекстЗапроса);
		//|WHERE DIRECT = 'O' AND DBSIGN = RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3)
		//|	AND DWNLDID <= RIGHT('      ' + RTRIM('" + _IdToStr(ВрмНомерПринятогоКорреспондентом) + "'), 6) + RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3);
		ВрмТекстЗапроса = "
		|INSERT INTO _1SDWNLDS 
		|	SELECT
		|		RIGHT('      ' + RTRIM('" + _IdToStr(ВхПакетНомер) + "'), 6) + DBSIGN DWNLDID,
		|		DBSIGN,
		|		'I' DIRECT,
		|		'A' ACKNOWL
		|	FROM _1SDBSET
		|	WHERE
		|		DBSIGN == RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3);
		|";

	ЗаписатьВПротоколВыполнения(Шаблон("Функция: {ЗакончитьЧтениеСообщения} Запрос: {[ВрмТекстЗапроса]}"), , 0, 1, 7);
	
