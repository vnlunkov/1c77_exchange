	Для ВрмИндекс = 1 По ВрмПараметры.РазмерСписка() Цикл
		ВрмПараметр = "";
		ВрмЗначенние = ВрмПараметры.ПолучитьЗначение(ВрмИндекс, ВрмПараметр);
		ВрмСтрока = СтрЗаменить(ВрмСтрока, "[" + ВрмПараметр + "]", Строка(ВрмЗначенние));
	КонецЦикла;
	
	Возврат ВрмСтрока;
	
