Процедура ИнициализироватьГлобальныеПеременные(ВхГлобальныеПеременные)

	Если ТипЗначенияСтр(ВхГлобальныеПеременные) <> "СписокЗначений" Тогда
		Возврат;
	КонецЕсли;

	Для ВрмИндекс = 1 По ВхГлобальныеПеременные.РазмерСписка() Цикл
		ВрмПредставление = "";
		ВрмЗначение = ВхГлобальныеПеременные.ПолучитьЗначение(ВрмИндекс, ВрмПредставление);
		Шаблон("[УстановитьПеременную(" + ВрмПредставление + ",  ВрмЗначение)]");
	КонецЦикла;

КонецПроцедуры

Процедура ПриОткрытии()

	СтатусВозврата(0);

	ВрмФормаПараметры = СоздатьОбъект("СписокЗначений");
	ВрмПараметрыКоманды = СоздатьОбъект("СписокЗначений");

	Если ПустоеЗначение(Форма.Параметр) = 1 Тогда           
		Сообщить("Служебная обработка. Не для интерактивного режима.");
		Возврат;
	ИначеЕсли Форма.Параметр = "ВыполнитьОбмен" Тогда
		ВрмКоманда = Форма.Параметр;
	ИначеЕсли ТипЗначенияСтр(Форма.Параметр) = "СписокЗначений" Тогда
		ВрмФормаПараметры = Форма.Параметр;
		ВрмПараметрыКоманды = ВрмФормаПараметры.Получить("ПараметрыКоманды");
		ВрмКоманда = ВрмФормаПараметры.Получить("Команда");
	Иначе
		Сообщить("Не корректно передан параметр.");
		Возврат;
	КонецЕсли;

	ВрмИнициализация =  ИнициализироватьКомпоненты();

	Если ПустоеЗначение(Форма.Параметр) = 1 Тогда           
		Сообщить("Не проведена инициализация подсистемы.");
		Возврат;
	КонецЕсли;
	
	ВрмСтатусВозврата = 0;
	
	ВрмУзелОбменаИдентификатор = ВрмПараметрыКоманды.Получить("УзелОбменаИдентификатор");
	ВрмГлобальныеПеременные = ВрмФормаПараметры.Получить("ГлобальныеПеременные");

	ИнициализироватьГлобальныеПеременные(ВрмГлобальныеПеременные);
	ИнициализироватьВедениеПротоколаОбмена(ВрмУзелОбменаИдентификатор);
	
	

	Если ВрмКоманда = "ЗагрузитьНастройкиОбмена" Тогда
		ВрмРезультат = ЗагрузитьНастройкиОбмена();
		ВрмФормаПараметры.Установить("НастройкиОбмена", мНастройкиОбмена);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
		// ВрмФормаПараметры.Установить("СтатусВозврата", ВрмСтатусВозврата);
	ИначеЕсли ВрмКоманда = "ЗаполнитьСписокУзловИзБД" Тогда
		ВрмРезультат = ЗаполнитьСписокУзловИзБД();
		ВрмФормаПараметры.Установить("НастройкиОбмена", мНастройкиОбмена);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
	ИначеЕсли ВрмКоманда = "СохранитьНастройкиОбмена" Тогда
		ВрмСтатусВозврата = СохранитьНастройкиОбмена(ВрмПараметрыКоманды.Получить("НастройкиОбмена"));
		//TODO Привести к стандарту параметров функции
		ВрмФормаПараметры.Установить("СтатусВозврата", ВрмСтатусВозврата);
	ИначеЕсли ВрмКоманда = "АктивироватьУзелОбмена" Тогда
		ВрмСписокПараметры = ВрмПараметрыКоманды.Получить("СписокПараметры");
		ВрмРезультат = АктивироватьУзелОбмена(ВрмПараметрыКоманды.Получить("УзелОбменаИдентификатор"), ВрмСписокПараметры);
		ВрмФормаПараметры.Установить("СписокПараметры", ВрмСписокПараметры);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
	ИначеЕсли ВрмКоманда = "ЗарегистрироватьПоФайлу" Тогда
		ВрмСписокПараметры = ВрмПараметрыКоманды.Получить("СписокПараметры");
		ВрмФайл = ВрмПараметрыКоманды.Получить("Файл");
		ВрмРезультат = ЗарегистрироватьПоФайлу(ВрмПараметрыКоманды.Получить("УзелОбменаИдентификатор"), ВрмФайл);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
	ИначеЕсли ВрмКоманда = "ПолучитьСтруктуруПравилОбмена" Тогда
		ВрмВозврат = ПолучитьСтруктуруПравилОбмена(ВрмПараметрыКоманды.Получить("Источник"));
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмВозврат);
		// Возврат;
	ИначеЕсли ВрмКоманда = "ВыполнитьВыгрузкуДанных" Тогда
		ВрмСписокПараметры = ВрмПараметрыКоманды.Получить("СписокПараметры");
		// мНастройкиОбмена = ВрмПараметрыКоманды.Получить("НастройкиОбмена");
		ВрмРезультат = ВыполнитьВыгрузкуДанных(ВрмПараметрыКоманды.Получить("УзелОбменаИдентификатор"), ВрмСписокПараметры);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
	ИначеЕсли ВрмКоманда = "ВыполнитьОбмен" Тогда
		
		ВрмУзелОбменаИдентификатор = "";
		ВрмСписокПараметры = СоздатьОбъект("СписокЗначений");
		ВрмРезультат = ЗагрузитьНастройкиОбмена();
		//ВрмФормаПараметры.Установить("НастройкиОбмена", мНастройкиОбмена);
		//ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
		ВрмРезультат.Установить("СтатусВозврата", 0);

		ВрмИндексТекущегоУзлаОбмена = 0;
		ВрмСписокУзловОбмена = мНастройкиОбмена.Получить("УзлыОбмена");
		Для ВрмИндекс = 1 По ВрмСписокУзловОбмена.РазмерСписка() Цикл
			
			ВрмТекущийУзелОбменаИдентификатор = "";	
			ВрмТекущийСписокПараметры = ВрмСписокУзловОбмена.ПолучитьЗначение(ВрмИндекс, ВрмТекущийУзелОбменаИдентификатор);
	
			Если Число(ВрмТекущийСписокПараметры.Получить("selected")) = 1 Тогда
				ВрмУзелОбменаИдентификатор = ВрмТекущийУзелОбменаИдентификатор;
				ВрмСписокПараметры = ВрмТекущийСписокПараметры;
				ВрмРезультат.Установить("СтатусВозврата", 1);
			КонецЕсли;
	
		КонецЦикла;

		мВыводСообщений = 1;
		
		Если ПолучитьСтатусВозврата(ВрмРезультат) = 1 Тогда
			мРежимОбмена = "Загрузка";
			ВрмРезультат = ВыполнитьЗагрузкуДанных(ВрмУзелОбменаИдентификатор, ВрмСписокПараметры);
		КонецЕсли;

		Если ПолучитьСтатусВозврата(ВрмРезультат) = 1 Тогда
			мРежимОбмена = "Выгрузка";
			мНеВыгружатьОбъектыПоСсылкам = 1;
			ВрмРезультат = ВыполнитьВыгрузкуДанных(ВрмУзелОбменаИдентификатор, ВрмСписокПараметры);
		КонецЕсли;
		
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");

	ИначеЕсли ВрмКоманда = "ВыполнитьЗагрузкуДанных" Тогда
		ВрмСписокПараметры = ВрмПараметрыКоманды.Получить("СписокПараметры");
		// мНастройкиОбмена = ВрмПараметрыКоманды.Получить("НастройкиОбмена");
		ВрмРезультат = ВыполнитьЗагрузкуДанных(ВрмПараметрыКоманды.Получить("УзелОбменаИдентификатор"), ВрмСписокПараметры);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
		// Возврат;
	ИначеЕсли ВрмКоманда = "ТестСоединения" Тогда
		ВрмСписокПараметры = ВрмПараметрыКоманды.Получить("СписокПараметры");
		ВрмРезультат = ТестСоединения(ВрмПараметрыКоманды.Получить("URLВебСервиса"), ВрмСписокПараметры);
		ЗаполнитьЗначенияСвойств(ВрмФормаПараметры, ВрмРезультат, "Результат");
	Иначе
		ВрмФормаПараметры.Установить("СтатусВозврата", 0);
		ВрмФормаПараметры.Установить("ОписаниеОшибки", "Команда [" + ВрмКоманда + "] отсутствет.");
	КонецЕсли;

	Если ПолучитьСтатусВозврата(ВрмФормаПараметры) = 0 Тогда
		ЗаписатьВПротоколВыполнения("Вызов [" + ВрмКоманда + "]. " + ВрмФормаПараметры.Получить("ОписаниеОшибки"), , 1, 1, 7);
	КонецЕсли;

	Если ПолучитьСтатусВозврата(ВрмФормаПараметры) = 1 Тогда

	ИначеЕсли Форма.Параметр = "ВыполнитьОбмен" Тогда
		Сообщить("Вызов [" + ВрмКоманда + "] завершен с ошибкой. Смотри лог выполнения.");
	КонецЕсли;

	ЗавершитьВедениеПротоколаОбмена();

	//Сообщить(мИдентификаторЭтогоУзла);

КонецПроцедуры

