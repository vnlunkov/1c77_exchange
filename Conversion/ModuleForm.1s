Функция СтрНайтиСтроки(ВхСтрока, ВхПодстрока)
	
	ВрмВозврат = СоздатьОбъект("СписокЗначений");
	ВрмТаблица = СоздатьОбъект("ТаблицаЗначений");
	ВрмТаблица.НоваяКолонка("Name");

	ВрмСтрока = ВхСтрока;
	ВрмПодстрокаДлина = СтрДлина(ВхПодстрока);
	ВрмСловоНачало = 1;
	Пока ВрмСловоНачало > 0 Цикл
		ВрмСловоНачало = Найти(ВрмСтрока, ВхПодстрока);
		Если ВрмСловоНачало = 0 Тогда
			Продолжить;
		КонецЕсли;
		ВрмСтрока = Сред(ВрмСтрока, ВрмСловоНачало, СтрДлина(ВрмСтрока) - ВрмСловоНачало);
		ВрмСловоКонец = 0;
		Для ВрмПозиция = 1 По СтрДлина(ВрмСтрока) Цикл
			ВрмСимвол = Сред(ВрмСтрока, ВрмПозиция, 1);
			Если  ВрмСимвол = Симв(13) Тогда
				ВрмСловоКонец = ВрмПозиция;
				Прервать;
			ИначеЕсли  ВрмСимвол = " " Тогда
				ВрмСловоКонец = ВрмПозиция;
				Прервать;
			КонецЕсли;
		КонецЦикла;
		// ВрмСловоКонец = Найти(ВрмСтрока, " ");
		Если ВрмСловоКонец = 0 Тогда
			ВрмСловоКонец = СтрДлина(ВрмСтрока);
		КонецЕсли;
		ВрмТаблица.НоваяСтрока();
		ВрмТаблица.Name = Сред(ВрмСтрока, ВрмПодстрокаДлина + 1, ВрмСловоКонец - ВрмПодстрокаДлина - 1);
		// ВрмВозврат.ДобавитьЗначение(Сред(ВрмСтрока, ВрмПодстрокаДлина + 1, ВрмСловоКонец - ВрмПодстрокаДлина - 1));
		ВрмСтрока = Прав(ВрмСтрока, СтрДлина(ВрмСтрока) - ВрмСловоКонец + 1);
	Конеццикла;
	ВрмТаблица.Свернуть("Name", "");
	ВрмТаблица.Выгрузить(ВрмВозврат, , , "Name");
	Возврат ВрмВозврат;
	
КонецФункции

Функция ВыполнитьЗапросФайловаяБД(ВхТекстЗапроса)
	
	ВрмВозврат = ПолучитьПустоеЗначение();

	ВрмСписокТаблиц = СтрНайтиСтроки(ВхТекстЗапроса, "_1S");

	ВрмБД = СоздатьОбъект("SQLiteBase");
	ВрмБД.Открыть(":memory:");
	ВрмЗапрос = ВрмБД.НовыйЗапрос();

	Для ВрмИндекс = 1 По ВрмСписокТаблиц.РазмерСписка() Цикл
		ВрмТаблица = ВрмСписокТаблиц.ПолучитьЗначение(ВрмИндекс);
		ВрмТекстЗапроса = Шаблон("create virtual table _1S[ВрмТаблица] using dbeng(_1S.[ВрмТаблица])");
		Сообщить(ВрмТекстЗапроса);
		ВрмЗапрос.ВыполнитьЗапрос(ВрмТекстЗапроса);
	КонецЦикла;
	ВрмТекстЗапроса = СтрЗаменить(ВхТекстЗапроса, "SUBSTRING(", "SUBSTR(");
	ВрмТекстЗапроса = СтрЗаменить(ВрмТекстЗапроса, "LEN(", "LENGTH(");
	ВрмТекстЗапроса = СтрЗаменить(ВрмТекстЗапроса, "+", "||");
	

	Сообщить(ВрмТекстЗапроса);
	ВрмВозврат = ВрмЗапрос.ВыполнитьЗапрос(ВрмТекстЗапроса);
	
	Если ВрмБД.Открыта() = 1 Тогда
		ВрмБД.Закрыть();
	КонецЕсли;
	ВрмЗапрос = ПолучитьПустоеЗначение();
	ВрмБД = ПолучитьПустоеЗначение();

	Возврат ВрмВозврат;

КонецФункции

Функция ВыполнитьЗапросOLEDB(ВхТекст)
	
	ВрмВозврат = ПолучитьПустоеЗначение();

	ВрмБД = СоздатьОбъект("OLEDBData");
    Соединение = "Provider=VFPOLEDB.1;Data Source=" + КаталогИБ() + ";Mode=ReadWrite;Collating Sequence=MACHINE";
	Попытка
		Рез = ВрмБД.Соединение(Соединение);	
	Исключение
		ВрмБД = ПолучитьПустоеЗначение();
		Возврат ВрмВозврат;	
	КонецПопытки;

	ВрмТекстЗапроса = СтрЗаменить(ВхТекст, "_1S", "1S");
	//ВрмТекстЗапроса = СтрЗаменить(ВрмТекстЗапроса, " = ", " == ");
	Сообщить(ВрмТекстЗапроса);
	//Попытка
		ВрмЗапрос = ВрмБД.СоздатьКоманду();
		ВрмЗапрос.Выполнить("EXECSCRIPT('SET ANSI OFF')");
		ВрмЗапрос.Выполнить("EXECSCRIPT('SET REPROCESS TO 60 SECONDS')");
		ВрмЗапрос.Выполнить("EXECSCRIPT('SET REFRESH TO 0,-1')");    
		ВрмЗапрос.Выполнить("Exec('SET TABLEVALIDATE TO 0')");	
		ВрмВозврат = ВрмЗапрос.ВыполнитьИнструкцию(ВрмТекстЗапроса);
	//Исключение

	//КонецПопытки;
	ВрмЗапрос.Закрыть();
	ВрмБД.Закрыть();
	ВрмЗапрос = ПолучитьПустоеЗначение();
	ВрмБД = ПолучитьПустоеЗначение();


	Сообщить(Шаблон("{[ТипЗначенияСтр(ВрмВозврат)]}{[ВрмТекстЗапроса]}"));
	Возврат ВрмВозврат;
КонецФункции

