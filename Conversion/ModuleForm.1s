	ТипИсточника          = ПолучитьРеквизитПКО(НомерПКО, "Источник");
	ТипПриемника          = ПолучитьРеквизитПКО(НомерПКО, "Приемник");
	
	ЗаписатьВФайлУдалениеОбъекта(Источник, ТипПриемника, ТипИсточника);

