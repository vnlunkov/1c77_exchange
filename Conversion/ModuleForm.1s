	ВрмВозврат.Установить("СтатусВозврата", 1);
	
	// Если ВрмЗапросСКЛ.ВыборкаОткрыта() = 1 Тогда
	// 	ВрмЗапросСКЛ.Закрыть();
	// КонецЕсли;
	// ВрмЗапросСКЛ = ПолучитьПустоеЗначение();

	Возврат ВрмВозврат;
	
КонецФункции

Функция АктивироватьУзелОбмена(ВхУзелОбменаИдентификатор, ВхСписокПараметры)
	
	ВрмВозврат = СоздатьОбъект("СписокЗначений");
	ВрмСтатусВозврата = 0;

	ВрмURLВебСервиса = ВхСписокПараметры.Получить("soap_uri");

	Если ПустоеЗначение(ВрмURLВебСервиса) = 1 Тогда
		ВрмВозврат.Установить("СтатусВозврата", ВрмСтатусВозврата);
		ВрмВозврат.Установить("ОписаниеОшибки", "Не настроено подключение к удаленному узлу!");
		Возврат ВрмВозврат;
	КонецЕсли;

	//ВрмЗапросСКЛ = ПолучитьCOMОбъект("ODBCRecordset");

	//ВрмТекстЗапроса = "UPDATE _1SDBSET SET DBSTATUS = 'C' WHERE RTRIM(DBSIGN) = '" + ВхУзелОбменаИдентификатор + "'";// AND DBSTATUS != 'C'";
	ВрмТекстЗапроса = "
		|UPDATE
		|	_1SDBSET
		|SET DBSTATUS = 'C'
		|WHERE DBSIGN == RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3) AND DBSTATUS != 'C'";

	ВрмСтатусВозврата = ВыполнитьЗапросБД(ВрмТекстЗапроса);
	Сообщить(ВрмСтатусВозврата);
	Если ВрмСтатусВозврата = 1 Тогда
		Сообщить("Инициализация узла с кодом [" + ВхУзелОбменаИдентификатор + "] успешна завершена.");
		Предупреждение("Необходимо перезапустить ИБ!");
	Иначе
		Сообщить("Узлу с кодом [" + ВхУзелОбменаИдентификатор + "] не требуется активация.");
	КонецЕсли;

	ВрмТекстЗапроса = "
		|INSERT INTO
		|	_1SDWNLDS 
		|	SELECT
		|		'     0' + DBSIGN DWNLDID,
		|		DBSIGN,
		|		'O' DIRECT,
		|		'A' ACKNOWL
		|	FROM _1SDBSET
		|	WHERE
		|		DBSIGN == RIGHT('   ' + '" + ВхУзелОбменаИдентификатор + "', 3) AND DBSTATUS != 'C'";

	//Сообщить(ВрмТекстЗапроса);

	//ВрмСтатусВозврата = ВрмЗапросСКЛ.ВыполнитьИнструкцию(ВрмТекстЗапроса);
	ВрмСтатусВозврата = ВыполнитьЗапросБД(ВрмТекстЗапроса);

	Если ПустоеЗначение(ВрмСтатусВозврата) = 1 Тогда
		Сообщить("Инициализацирована таблица отправки.");
	Иначе
		Сообщить("Не удалось инициализировать таблицу отправки.");
	КонецЕсли;

	//Если ВрмЗапросСКЛ.ВыборкаОткрыта() = 1 Тогда
	//	ВрмЗапросСКЛ.Закрыть();
	//КонецЕсли;
	//ВрмЗапросСКЛ = ПолучитьПустоеЗначение();

	ВрмПараметры = СоздатьОбъект("СписокЗначений");
	//ВрмПараметры.Установить("ИмяПланаОбмена", "СН_Обмен_77"); //ВхСписокПараметры.Получить();
	ВрмПараметры.Установить("ИмяПланаОбмена", "СН_ОбменУправлениеХолдингомУчетныеСистемы");
	ВрмПараметры.Установить("ПрефиксБазыИсточника", мНастройкиОбмена.Получить("id"));
	ВрмПараметры.Установить("ПрефиксБазыПриемника", ВхСписокПараметры.Получить("id_self_code"));
	ВрмПараметры.Установить("ИмяБазыПриемника", ВхСписокПараметры.Получить("id_self_code"));

	ВрмТелоЗапроса = ПолучитьТекстЗапроосаИзШаблона("CreateExchangeNode", "3_0_1_1", ВрмПараметры);
	ВрмРезультатЗапроса = ЗапросSOAP(ВрмURLВебСервиса, "Exchange_3_0_1_1", ВрмТелоЗапроса);

	Если ПолучитьСтатусВозврата(ВрмРезультатЗапроса) = 0 Тогда
		Возврат ВрмРезультатЗапроса;
	КонецЕсли;

	ВрмОтветSOAP = ОбработатьОтветSOAP(ВрмРезультатЗапроса.Получить("Результат"), "CreateExchangeNodeResponse");

	Если ПолучитьСтатусВозврата(ВрмОтветSOAP) = 0 Тогда
		Возврат ВрмОтветSOAP;
	КонецЕсли;
	
	ВрмСтатусВозврата = 1;

	ВхСписокПараметры.Установить("active", ВрмСтатусВозврата);
	// ВхСписокПараметры.Установить("path", ВрмКаталогОбменаУзла);

	

	ВрмВозврат.Установить("СтатусВозврата", ВрмСтатусВозврата);
	Возврат ВрмВозврат;
	
КонецФункции

Функция ЗарегистрироватьПоФайлу(ВхУзелОбменаИдентификатор, ВхФайл)
	
	ВрмВозврат = СоздатьОбъект("СписокЗначений");
	

	xmlDOM = createobject("MSXML2.DOMDocument");
	xmlDOM.load(ВхФайл);

	ВрмЗапрос = "";
	//nodesTable = xmlDOM.documentElement.selectNodes("//table");
	nodesTable = xmlDOM.selectNodes("//table");
	Для idxTable = 0 По nodesTable.length - 1 Цикл
		
		nodeTable = nodesTable.item(idxTable);
		ВрмТип = "SC";
		ВрмСписокИдентификаторов = "";
		// ВрмТип = nodeTable.getAttribute("type");
		ВрмВид = nodeTable.getAttribute("kind");
		
		nodesRecord = nodeTable.selectNodes("//record");
		Для idxRecord = 0 По nodesRecord.length - 1 Цикл
			nodeRecord = nodesRecord.item(idxRecord);
			_id = nodeRecord.getAttribute("id");
			ВрмУзелИд = Прав(_id, 3);
			ВрмОбъектИд = _IdToStr(Лев(_id, 10));
			ВрмИД = Прав("      " + ВрмОбъектИд, 6) + ВрмУзелИд;
			ВрмСписокИдентификаторов = ВрмСписокИдентификаторов + ?(СтрДлина(ВрмСписокИдентификаторов) = 0, "", ",") + "'" + ВрмИД + "'";
		КонецЦикла;
		
		
		ВрмЗапрос = ВрмЗапрос + ?(СтрДлина(ВрмЗапрос) = 0, "", "	UNION ") + Шаблон("
		|	SELECT
		|		RIGHT('   ' + '[ВхУзелОбменаИдентификатор]', 3) DBSIGN,
		|		[ВрмВид] TYPEID,
		|		ID OBJID,
		|		'' DELETED,
		|		'' DWNLDID
		|	FROM [ВрмТип][ВрмВид] WHERE ID IN ([ВрмСписокИдентификаторов]) AND ID NOT IN (SELECT OBJID FROM _1SUPDTS WHERE DBSIGN = RIGHT('   ' + '[ВхУзелОбменаИдентификатор]', 3) AND TYPEID = [ВрмВид])
		|");

	КонецЦикла;

	ВрмЗапросСКЛ = ПолучитьCOMОбъект("ODBCRecordset");

	ВрмТекстЗапроса = Шаблон("
		|INSERT INTO _1SUPDTS 
		|	[ВрмЗапрос]
		|;
		|");

	// Сообщить(ВрмТекстЗапроса);

	//  Возврат ВрмВозврат;
	ЗаписатьВПротоколВыполнения(Шаблон("Функция: {ЗарегистрироватьПоФайлу} Запрос: {[ВрмТекстЗапроса]}"), , 0, 1, 7);
	ВрмСтатусВозврата = ВрмЗапросСКЛ.ВыполнитьИнструкцию(ВрмТекстЗапроса);
	Если ВрмСтатусВозврата = 1 Тогда
		ВрмВозврат.Установить("Регистрация объектов для [" + ВхУзелОбменаИдентификатор + "] успешна завершена.");
	Иначе
		ВрмВозврат.Установить("ОписаниеОшибки", "Ошибка регистрации объектов для [" + ВхУзелОбменаИдентификатор + "]");
	КонецЕсли;
	ВрмВозврат.Установить("СтатусВозврата", ВрмСтатусВозврата);

	Если ВрмЗапросСКЛ.ВыборкаОткрыта() = 1 Тогда
		ВрмЗапросСКЛ.Закрыть();
	КонецЕсли;
	ВрмЗапросСКЛ = ПолучитьПустоеЗначение();

	Возврат ВрмВозврат;
	
КонецФункции

