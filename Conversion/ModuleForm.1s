//*****************************************************************************
Процедура ЗаписатьСоответствие(пСсылка,пИдентификатор8)
	
	ФайлДбф = СоздатьОбъект("XBASE");

	ВрмИдентификатор = "";

	Если ФС.СуществуетФайл(мИмяФайлаРегистрСоответствия) = 1 Тогда
		Если ФайлДбф.Открыта() = 0 Тогда
			ФайлДбф.ОткрытьФайл(мИмяФайлаРегистрСоответствия, мИмяФайлаИндексаРегистрСоответствия,0);
		КонецЕсли;
		
		ФайлДбф.ТекущийИндекс("idxref8");
		Если ФайлДбф.Найти(пИдентификатор8,0) = 1 Тогда
			ВрмИдентификатор = ПолучитьСсылкуПоИдентификатору(ФайлДбф.ref77);
		КонецЕсли;
		
	КонецЕсли;

	Если ПустаяСтрока(ВрмИдентификатор) = 1 Тогда
	
		ФайлДбф.Добавить();
		ФайлДбф.ref77 = ПолучитьУникальныйИдентификатор(пСсылка);
		ФайлДбф.ref8 = пИдентификатор8;
		ФайлДбф.Записать();

	КонецЕсли;

	ФайлДбф.ЗакрытьФайл();
	ФайлДбф = ПолучитьПустоеЗначение();

КонецПроцедуры                         

