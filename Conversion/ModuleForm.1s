				Имя = мЧтениеXML.СвойстваТекущегоУзла.Имя;
				
					Если Имя = "Источник" Тогда
						
						Узел = мЧтениеXML.ТекущийЭлементВВидеОбъекта();
						Источник = Узел.Значение;
					
					ИначеЕсли Имя = "Приемник" Тогда
						
						Узел = мЧтениеXML.ТекущийЭлементВВидеОбъекта();
						Приемник = Узел.Значение;
					
					КонецЕсли;
				
