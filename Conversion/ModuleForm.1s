			message("[" + ВрмИмяУзла + "][" + ВрмУзелПараметр.text + "]");
			
		КонецЕсли;
		
	КонецЦикла;

	xmlDOM = ПолучитьПустоеЗначение();
	// ВрмПарсерХМЛ = ПолучитьПустоеЗначение();
	
	Возврат ВрмВозврат;
	
