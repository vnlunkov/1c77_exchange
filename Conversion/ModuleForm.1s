	ВрмПутьФайлСообщения = Шаблон("[ВрмПутьКаталогСообщения]Message{[ВрмИдСообщения]}.xml");
	
	Если ФС.СуществуетФайл(ВрмПутьКаталогИсходящихСообщений) = 0 Тогда
		ФС.СоздатьКаталог(ВрмПутьКаталогИсходящихСообщений);
