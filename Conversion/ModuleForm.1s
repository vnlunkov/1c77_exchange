		ВрмСписокПараметры.Установить("id_self_code", СокрЛП(ВрмТаблицаУзлыОбмена.DBSELF));
		ВрмСписокПараметры.Установить("id", СокрЛП(ВрмТаблицаУзлыОбмена.DBSIGN));
		ВрмСписокПараметры.Установить("active", ВрмСтатусУзла);
		ВрмСписокПараметры.Установить("uuid", ВрмТаблицаУзлыОбмена.DBUUID);

		ВрмСписокУзловОбмена.Установить(СокрЛП(ВрмТаблицаУзлыОбмена.DBSIGN), ВрмСписокПараметры);
		
	КонецЦикла;

	ВрмТаблицаУзлыОбмена = ПолучитьПустоеЗначение();

	мНастройкиОбмена.Установить("СохранитьНастройки", 1);
